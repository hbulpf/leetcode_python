    "class Solution(object):\n",
    "    def isPalindrome(self, s):\n",
    "        \"\"\"\n",
    "        :type s: str\n",
    "        :rtype: bool\n",
    "        \"\"\"\n",
    "        s = s.lower()\n",
    "        s = list(filter(lambda x: x.isalpha() or x.isdigit(), s))\n",
    "        if len(s) == 0:\n",
    "            return True\n",
    "        return s == s[::-1]"
   ]
  }
