    "class Solution:\n",
    "    def mySqrt(self, x):\n",
    "        \"\"\"\n",
    "        :type x: int\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        if x <= 1:\n",
    "            return x\n",
    "        for i in range(1,x+1):\n",
    "            if i ** 2 > x:\n",
    "                break\n",
    "        return i-1"
   ]
