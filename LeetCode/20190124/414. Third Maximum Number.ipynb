    "class Solution:\n",
    "    def thirdMax(self, nums):\n",
    "        \"\"\"\n",
    "        :type nums: List[int]\n",
    "        :rtype: int\n",
    "        \"\"\"\n",
    "        s = set(nums)\n",
    "        nums = list(s)\n",
    "        nums.sort()\n",
    "        if len(nums) < 3:\n",
    "            return nums[-1]\n",
    "        return(nums[-3])"
   ]
